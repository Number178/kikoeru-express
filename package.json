{
  "name": "kikoeru-express",
  "version": "0.6.0-rc.4",
  "description": "server",
  "engines": {
    "node": ">=12.0.0"
  },
  "bin": {
    "kikoeru-express": "app.js"
  },
  "pkg": {
    "scripts": [
      "filesystem/scanner.js",
      "dist/**/*.js",
      "database/migrations/*",
      "database/knexfile.js",
      "database/storage.js"
    ],
    "assets": [
      "dist/**/*",
      "static/**/*"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "config/*",
      "sqlite/*"
    ],
    "delay": "2500"
  },
  "scripts": {
    "start": "node --trace-warnings app.js",
    "dev": "nodemon --trace-warnings app.js",
    "scan": "node --trace-warnings ./filesystem/scanner.js",
    "build": "pkg -t win package.json --out-path package && pkg -t macos package.json --out-path package-macos",
    "test": "eslint ."
  },
  "author": "Watanuki-Kimihiro",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@umonaca/umzug": "^2.3.1",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.5",
    "compare-versions": "^3.6.0",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-validator": "^6.9.2",
    "htmlparser2": "^4.1.0",
    "https": "^1.0.0",
    "invariant": "^2.2.4",
    "jschardet": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.17",
    "knex-migrate": "^1.7.4",
    "limit-promise": "^1.0.4",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "natural-orderby": "^2.0.3",
    "recursive-readdir": "^2.2.2",
    "socket.io": "^2.4.1",
    "socketio-jwt-auth": "^0.2.1",
    "sqlite3": "^5.0.2",
    "string-random": "^0.1.3",
    "tunnel-agent": "^0.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-plugin-node": "^11.1.0"
  }
}

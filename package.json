{
  "name": "kikoeru-express",
  "version": "0.5.2-alpha",
  "description": "server",
  "bin": "app.js",
  "pkg": {
    "scripts": [
      "filesystem/scanner.js",
      "dist/**/*.js"
    ],
    "assets": [
      "dist/**/*",
      "static/**/*",
      "database/migrations/*",
      "database/knexfile.js",
      "database/storage.js"
    ]
  },
  "nodemonConfig": {
    "ignore": ["config/*", "sqlite/*"],
    "delay": "2500"
  },
  "scripts": {
    "start": "node --trace-warnings app.js",
    "dev": "nodemon --trace-warnings app.js",
    "scan": "node --trace-warnings ./filesystem/scanner.js",
    "build": "pkg -t win package.json --out-path package",
    "test": "node_modules/.bin/eslint ."
  },
  "author": "Watanuki-Kimihiro",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "compare-versions": "^3.6.0",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-validator": "^6.4.0",
    "htmlparser2": "^4.1.0",
    "jschardet": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.16",
    "knex-migrate": "^1.7.4",
    "limit-promise": "^1.0.4",
    "md5": "^2.2.1",
    "natural-orderby": "^2.0.3",
    "recursive-readdir": "^2.2.2",
    "socket.io": "^2.3.0",
    "socketio-jwt-auth": "^0.1.0",
    "sqlite3": "^4.1.1",
    "string-random": "^0.1.3",
    "tunnel-agent": "^0.6.0"
  },
  "devDependencies": {
    "eslint": "^7.17.0"
  }
}
